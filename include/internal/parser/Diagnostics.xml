<!--
Diagnostic error definitions for the various components involved in compilation.
-->
<diagnostics>
  <component name="parser">
    <error id="InternalError">
      <descr>Internal error: {0}</descr>
    </error>
    <error id="UnexpectedToken">
      <descr>Unexpected token: got {0}, expected {1}</descr>
    </error>
    <error id="ExpectedIdentifier">
      <descr>Expected Identifier, got {0}</descr>
    </error>
    <error id="ExpectedSemicolon">
      <descr>Expected ';', got {0}</descr>
    </error>
    <error id="ExpectedTypeOrIdentifier">
      <descr>Expected type or Identifier, got {0}</descr>
    </error>
    <error id="LoneArrayBracket">
      <descr>Lone array bracker: {0}</descr>
    </error>
    <error id="ExpectedAssign">
      <descr>Expected '=', got {0}</descr>
    </error>
    <error id="ExpectedParenthesis">
      <descr>Expected {1}, got {0}</descr>
    </error>
    <error id="ExpectedBracket">
      <descr>Expected {1}, got {0}</descr>
    </error>
    <error id="ExpectedBrace">
      <descr>Expected {1}, got {0}</descr>
    </error>
    <error id="ExpectedTernaryOperator">
      <descr>Expected {1}, got {0}</descr>
    </error>
    <error id="ExpectedSeparatorOrParenthesis">
      <descr>Expected ',' or ')', got {0}</descr>
    </error>
    <error id="ExpectedExpression">
      <descr>Expected expression, got {0}</descr>
    </error>
    <error id="IdentifierAlreadyDeclared">
      <descr>Identifier already declared</descr>
    </error>
    <error id="IdentifierUndeclared">
      <descr>Identifier undeclared</descr>
    </error>
    <error id="DeclarationNotAllowedInScope">
      <descr>Declaration not allowed in this scope</descr>
    </error>
    <error id="ExpectedTypeName">
      <descr>Expected type name</descr>
    </error>
    <error id="OutParameterWithDefault">
      <descr>&lsquo;out&rsquo; parameter with default value</descr>
    </error>
    <error id="ConflictingQualifiersForInParam">
      <descr>Conflicting qualifiers for &lsquo;in&rsquo; parameter</descr>
    </error>
    <error id="QualifiersNotAllowed">
      <descr>Qualifiers not allowed here</descr>
    </error>
  </component>
</diagnostics>
